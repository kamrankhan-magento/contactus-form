<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">     
                <resource id="FME_Extensions::fmeextensions" title="FME Extensions">
                	<resource id="FME_Contactus::contactus_main" title="Contact us" sortOrder="30">
                		<resource id="FME_Contactus::contactus" title="Manage Submissions" sortOrder="30" />
                                <resource id="FME_Contactus::contactus_addform" title="Manage Fields" sortOrder="31" />
                                <resource id="FME_Contactus::contactus_conf" title="Contactus Configuration" sortOrder="32" />
                	</resource>                                                 
                </resource>        
            <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="FME_Contactus::contactus_configuration" title="Contactus Configuration"/>
                        </resource>
                    </resource>
            </resource>
	</resource>
        </resources>
    </acl>
</config>
